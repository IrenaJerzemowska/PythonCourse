atext = "this is a text."
atext.endswith('ext')
#powinno wypluć ze false

atext.endswith('ext.')
#powinno wypluc ze true

#funkcja sprawdzajaca czy text jest malymi literami
atext.islower()

#zmieniamy na duze litery
atext.upper()
'THIS IS A TEXT.'

atext.upper().isupper()
#ta funkcja wykryła, że tekst jest z wielkich liter

atext.find('is')
#ta funkcja wykrywa czy w tekscie znajduje sie slowo 'is'

atext.replace('a','1')

print (atext)

atext.replace('1','e',3)

print(atext)
